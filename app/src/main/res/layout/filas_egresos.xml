<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/mainLayoutFilaCompras">

    <com.google.android.material.card.MaterialCardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:cardBackgroundColor="@color/md_theme_surfaceContainer"
        app:cardElevation="1dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            android:paddingVertical="12dp"
            android:paddingHorizontal="16dp">

            <com.google.android.material.button.MaterialButton
                style="@style/Widget.Material3.Button.IconButton.Outlined"
                android:id="@+id/btnPuestoLineaMasVendido"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="11sp"
                android:textStyle="bold"
                android:layout_marginRight="6dp"
                app:strokeColor="@color/md_theme_outlineVariant"
                app:strokeWidth="2dp"
                app:icon="@drawable/north_east_24px">
            </com.google.android.material.button.MaterialButton>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/textView_Tipo_FilaEgresos"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Egreso"
                    android:textSize="18sp"
                    android:textStyle="bold"

                    android:textColor="@color/md_theme_onSurfaceVariant"
                    >
                </TextView>
                <TextView
                    android:id="@+id/textView_Fecha_FilaEgresos"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="12/05/2024"
                    android:textSize="11sp"
                    android:textStyle="normal"
                    android:textColor="@color/md_theme_onSurfaceVariant"
                    >
                </TextView>
                <TextView
                    android:id="@+id/textView_Id_FilaEgresos"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="#1"
                    android:textSize="12sp"
                    android:textStyle="bold"
                    android:visibility="gone"
                    android:textColor="@color/md_theme_onSurfaceVariant"
                    >
                </TextView>
            </LinearLayout>

            <!--
            <com.google.android.material.divider.MaterialDivider
                android:layout_width="2dp"
                android:layout_height="35dp"
                android:layout_marginRight="16dp">
            </com.google.android.material.divider.MaterialDivider>
            -->

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="$"
                android:textSize="18sp"
                android:textStyle="bold"
                android:textAlignment="center">
            </TextView>
            <TextView
                android:id="@+id/textView_Monto_FilaEgresos"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2000"
                android:textSize="18sp"
                android:textStyle="bold"
                android:textAlignment="center">
            </TextView>

        </LinearLayout>

    </com.google.android.material.card.MaterialCardView>

